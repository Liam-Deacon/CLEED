##############################################################################
#
# PROCESS HANDLING FUNCTIONS AND MACROS 
#
##############################################################################

!macro CheckForProcess processName gotoWhenFound gotoWhenNotFound
   Processes::FindProcess @DOLLAR@{processName}
   StrCmp $R0 "0" @DOLLAR@{gotoWhenNotFound} @DOLLAR@{gotoWhenFound}
!macroend

!macro ConfirmEndProcess processName
   MessageBox MB_YESNO|MB_ICONEXCLAMATION \
     "Found @DOLLAR@{processName} process(s) which need to be stopped.$\nDo you want the installer to stop these for you?" \
     IDYES process_@DOLLAR@{processName}_kill IDNO process_@DOLLAR@{processName}_ended
   process_@DOLLAR@{processName}_kill:
      DetailPrint "Killing @DOLLAR@{processName} processes."
      Processes::KillProcess @DOLLAR@{processName}
      Sleep 1500
      StrCmp $R0 "1" process_@DOLLAR@{processName}_ended
      DetailPrint "Process to kill not found!"
   process_@DOLLAR@{processName}_ended:
!macroend

!macro CheckAndConfirmEndProcess processName
   !insertmacro CheckForProcess @DOLLAR@{processName} 0 no_process_@DOLLAR@{processName}_to_end
   !insertmacro ConfirmEndProcess @DOLLAR@{processName}
   no_process_@DOLLAR@{processName}_to_end:
!macroend

!macro FileCopy FilePath TargetDir
  CreateDirectory `${TargetDir}`
  CopyFiles `${FilePath}` `${TargetDir}`
!macroend


