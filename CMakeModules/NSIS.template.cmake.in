Var UnPageUserAppDataDialog
Var UnPageUserAppDataCheckbox
Var UnPageUserAppDataCheckbox_State
Var UnPageUserAppDataEditBox

Function un.UnPageUserAppData
   !insertmacro MUI_HEADER_TEXT "Uninstall @CPACK@_PACKAGE_NAME@" "Remove @CPACK@_PACKAGE_NAME@'s data folder from your computer."
   nsDialogs::Create /NOUNLOAD 1018
   Pop $UnPageUserAppDataDialog

   @DOLLAR@{If} $UnPageUserAppDataDialog == error
      Abort
   @DOLLAR@{EndIf}

   @DOLLAR@{NSD_CreateLabel} 0 0 100% 12u "Do you want to delete @CPACK@_PACKAGE_NAME@'s data folder?"
   Pop $0

   @DOLLAR@{NSD_CreateText} 0 13u 100% 12u "$LOCALAPPDATA\@CPACK@_PACKAGE_NAME@"
   Pop $UnPageUserAppDataEditBox
   SendMessage $UnPageUserAppDataEditBox @DOLLAR@{EM_SETREADONLY} 1 0

   @DOLLAR@{NSD_CreateLabel} 0 46u 100% 24u "Leave unchecked to keep the data folder for later use or check to delete the data folder."
   Pop $0

   @DOLLAR@{NSD_CreateCheckbox} 0 71u 100% 8u "Yes, delete this data folder."
   Pop $UnPageUserAppDataCheckbox

   nsDialogs::Show
FunctionEnd

Function un.UnPageUserAppDataLeave
   @DOLLAR@{NSD_GetState} $UnPageUserAppDataCheckbox $UnPageUserAppDataCheckbox_State
FunctionEnd

Section Uninstall
   IfFileExists "$INSTDIR\@CPACK@_PACKAGE_NAME@-gui.exe" @CPACK@_PACKAGE_NAME@-gui_installed
      MessageBox MB_YESNO "It does not appear that @CPACK@_PACKAGE_NAME@ is installed in the directory '$INSTDIR'.$\r$\nContinue anyway (not recommended)?" IDYES @CPACK@_PACKAGE_NAME@-gui_installed
      Abort "Uninstall aborted by user"
