   DeleteRegKey HKCU "Software\Classes\CLEED_vertex_file" 
   DeleteRegKey HKCR "Software\Classes\CLEED_vertex_file"    
   
   ; Start menu shortcuts.
   !ifdef OPTION_SECTION_SC_START_MENU
      SetShellVarContext all
      RMDir /r "$SMPROGRAMS\@CPACK@_PACKAGE_NAME@"
      SetShellVarContext current
   !endif

   ; Desktop shortcut.
   !ifdef OPTION_SECTION_SC_DESKTOP
      IfFileExists "$DESKTOP\@CPACK@_PACKAGE_NAME@.lnk" 0 +2
         Delete "$DESKTOP\@CPACK@_PACKAGE_NAME@.lnk"
   !endif

   ; Quick Launch shortcut.
   !ifdef OPTION_SECTION_SC_QUICK_LAUNCH
      IfFileExists "$QUICKLAUNCH\@CPACK@_PACKAGE_NAME@.lnk" 0 +2
         Delete "$QUICKLAUNCH\@CPACK@_PACKAGE_NAME@.lnk"
   !endif

   ; Remove all the Program Files.
   RMDir /r $INSTDIR

   ; Uninstall User Data if option is checked, otherwise skip.
   @DOLLAR@{If} $UnPageUserAppDataCheckbox_State == @DOLLAR@{BST_CHECKED}
      RMDir /r "$LOCALAPPDATA\@CPACK@_PACKAGE_NAME@"
   @DOLLAR@{EndIf}

   ; make sure windows knows about the change
   SendMessage @DOLLAR@{HWND_BROADCAST} @DOLLAR@{WM_WININICHANGE} 0 "STR:Environment" /TIMEOUT=5000
   
   SetDetailsPrint textonly
   DetailPrint "Finished."
SectionEnd

##############################################################################
# #
# NSIS Installer Event Handler Functions #
# #
##############################################################################

Function .onInit
   !insertmacro INSTALLOPTIONS_EXTRACT "NSIS.InstallOptions.ini"

   ; Remove Quick Launch option from Windows 7, as no longer applicable - usually.
   @DOLLAR@{IfNot} @DOLLAR@{AtMostWinVista}
      SectionSetText @DOLLAR@{SEC_QUICK_LAUNCH} "Quick Launch Shortcut (N/A)"
      SectionSetFlags @DOLLAR@{SEC_QUICK_LAUNCH} @DOLLAR@{SF_RO}
      SectionSetInstTypes @DOLLAR@{SEC_QUICK_LAUNCH} 0
   @DOLLAR@{EndIf}

   @DOLLAR@{MementoSectionRestore}

   UAC_Elevate:
      !insertmacro UAC_RunElevated
      StrCmp 1223 $0 UAC_ElevationAborted ; UAC dialog aborted by user?
      StrCmp 0 $0 0 UAC_Err ; Error?
      StrCmp 1 $1 0 UAC_Success ;Are we the real deal or just the wrapper?
      Quit

   UAC_Err:
      MessageBox MB_ICONSTOP "Unable to elevate, error $0"
      Abort

   UAC_ElevationAborted:
      Abort

   UAC_Success:
      StrCmp 1 $3 +4 ;Admin?
      StrCmp 3 $1 0 UAC_ElevationAborted ;Try again?
      MessageBox MB_ICONSTOP "This installer requires admin access, try again"
      goto UAC_Elevate

   ; Prevent multiple instances.
   System::Call 'kernel32::CreateMutexA(i 0, i 0, t "@CPACK@_PACKAGE_NAME@-guiInstaller") i .r1 ?e'
   Pop $R0
   StrCmp $R0 0 +3
      MessageBox MB_OK|MB_ICONEXCLAMATION "The installer is already running."
      Abort

   ; Use available InstallLocation when possible. This is useful in the uninstaller
   ; via re-install, which would otherwise use a default location - a bug.
   ReadRegStr $R0 HKLM "Software\Microsoft\Windows\CurrentVersion\Uninstall\@CPACK@_PACKAGE_NAME@" "InstallLocation"
   StrCmp $R0 "" SkipSetInstDir
   StrCpy $INSTDIR $R0
   SkipSetInstDir:

   ; Shutdown @CPACK@_PACKAGE_NAME@ in case Add/Remove re-installer option used.
   Call Ensure@CPACK@_PACKAGE_NAME@Shutdown
FunctionEnd

