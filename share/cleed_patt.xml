<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_patt-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED pattern input file</comment>
    <icon>cleed_patt</icon>
    <glob pattern=".patt" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE PATT"/>
    </magic>
    <root-XML localName="cleed_patt" />
  </mime-type>
 </mime-info>