<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_par-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED parameter file</comment>
    <icon>cleed_par</icon>
    <glob pattern=".par" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED PARAMETER"/>
    </magic>
    <root-XML localName="cleed_par" />
  </mime-type>
 </mime-info>