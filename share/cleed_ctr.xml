<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_ctr-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED control file</comment>
    <icon>cleed_ctr</icon>
    <glob pattern=".ctr" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED CONTROL"/>
    </magic>
    <root-XML localName="cleed_ctr" />
  </mime-type>
 </mime-info>