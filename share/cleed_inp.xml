<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_inp-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED input file</comment>
    <icon>cleed_inp</icon>
    <glob pattern=".inp" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED INPUT"/>
    </magic>
    <root-XML localName="cleed_inp" />
  </mime-type>
 </mime-info>