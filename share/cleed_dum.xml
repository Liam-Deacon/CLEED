<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_dum-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED dummy file</comment>
    <icon>cleed_dum</icon>
    <glob pattern=".dum" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED DUMMY"/>
    </magic>
    <root-XML localName="cleed_dum" />
  </mime-type>
 </mime-info>