<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_phs-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED phase shifts file</comment>
    <icon>cleed_phs</icon>
    <glob pattern=".phs" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED PHASESHIFTS"/>
    </magic>
    <root-XML localName="cleed_phs" />
  </mime-type>
 </mime-info>