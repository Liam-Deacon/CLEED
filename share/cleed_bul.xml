<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_bul-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED bulk parameter input file</comment>
    <icon>cleed_bul</icon>
    <glob pattern=".bul" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED BULK"/>
    </magic>
    <root-XML localName="cleed_bul" />
  </mime-type>
 </mime-info>