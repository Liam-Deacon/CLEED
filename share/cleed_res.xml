<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_res-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED results file</comment>
    <icon>cleed_res</icon>
    <glob pattern=".res" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED RESULTS"/>
    </magic>
    <root-XML localName="cleed_res" />
  </mime-type>
 </mime-info>