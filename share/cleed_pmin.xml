<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_pmin-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED parameter minimised file</comment>
    <icon>cleed_pmin</icon>
    <glob pattern=".pmin" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE CLEED PARAMETER MIN"/>
    </magic>
    <root-XML localName="cleed_pmin" />
  </mime-type>
 </mime-info>