<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
 <mime-type type="application/x-cleed_latt-item">
    <sub-class-of type="text/xml"/>
    <sub-class-of type="application/xml"/>
    <comment>CLEED lattice input file</comment>
    <icon>cleed_latt</icon>
    <glob pattern=".latt" weight="50" />
    <magic priority="90">
          <match type="string" offset="2" value="!DOCTYPE LATT"/>
    </magic>
    <root-XML localName="cleed_latt" />
  </mime-type>
 </mime-info>